		VirtualMachineError [label = "{<f0>VirtualMachineError|<f1>|<f2>}", style = "rounded, filled, dashed", fillcolor = "springgreen"];
	}
	
	subgraph cluster75 {
		label = "exception";
		style = filled;
		color = grey90;
		
		Exception [label = "{<f0>Exception|<f1>|<f2>}", style = "rounded, filled", fillcolor = "springgreen"];
		InvocationTargetException [label = "{<f0>InvocationTargetException|<f1>|<f2>}", style = "rounded, filled", fillcolor = "springgreen"];
		ReflectiveOperationException [label = "{<f0>ReflectiveOperationException|<f1>|<f2>}", style = "rounded, filled", fillcolor = "springgreen"];
	}
	
