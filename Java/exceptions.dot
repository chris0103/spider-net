digraph exceptions {
	rankdir = BT;
    fontname = "Verdana";
    fontsize = 12;
    node [fontname = "Verdana", fontsize = 12, shape = "record"];
    edge [fontname = "Verdana", fontsize = 12];
	

	Throwable [label = "{<f0>Throwable|<f1>+ cause : Throwable\l+ detailMessage : String|<f2>}", style = "rounded, filled", fillcolor = "springgreen"];	
	
	subgraph cluster25 {
		label = "error";
		style = filled;
		color = grey90;
		
		Error [label = "{<f0>Error|<f1>|<f2>}", style = "rounded, filled", fillcolor = "springgreen"];
		OutOfMemoryError [label = "{<f0>OutOfMemoryError|<f1>|<f2>}", style = "rounded, filled", fillcolor = "springgreen"];
